     {
       "version": 2,
       "builds": [
         {
           "src": "./pages/api/grade.js",
           "use": "@vercel/node"
         }
       ],
       "routes": [
         {
           "src": "/(.*)",
           "dest": "./index.js",
           "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
           "headers": {
             "Access-Control-Allow-Origin": "*",
             "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept, Origin"
           }
         }
       ]
     }
